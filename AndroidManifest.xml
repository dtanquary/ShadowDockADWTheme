<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.dtanquary.shadowdockadwtheme"
      android:installLocation="internalOnly" android:versionCode="1" android:versionName="1.0">
    <application android:label="@string/theme_title" android:icon="@drawable/app_theme_icon">
        <activity android:name=".main"
                  android:label="@string/theme_title"
                  android:icon="@drawable/app_theme_icon">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="org.adw.launcher.THEMES" />
                <category android:name="android.intent.category.DEFAULT" />                
            </intent-filter>
            <intent-filter>
                <action android:name="org.adw.launcher.icons.ACTION_PICK_ICON" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>              
        </activity>
    </application>
	<uses-sdk android:minSdkVersion="8"/>

</manifest>